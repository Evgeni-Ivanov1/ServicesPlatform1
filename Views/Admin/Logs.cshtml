@using ServicesPlatform.Data.Models
@model IEnumerable<AdminLog>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Admin Logs</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Action</th>
            <th>Performed By</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.Id</td>
                <td>@log.Action</td>
                <td>@log.PerformedBy</td>
                <td>@log.PerformedOn.ToString("g")</td>
            </tr>
        }
    </tbody>
</table>

<div>
    <a class="btn btn-primary" asp-controller="Admin" asp-action="Dashboard">Back to Dashboard</a>

</div>
